/* Force int32 to be 4 bytes */
#if (SIZEOF_LONG == 8)
#define int32 int
#define u_int32 unsigned int
#else
#define int32 long
#define u_int32 unsigned long
#endif

/* Set up prototype macro */
#ifndef PROTO
#ifdef __STDC__
#define PROTO(x) x
#else
#define PROTO(x) ()
#endif
#endif

/* config.h.bot - end */
